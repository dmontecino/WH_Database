# Arthropod Source

data_dictionary[["Arthopod_Source"]]<-
  
  list(
    
    data.frame(
      Variable="ArthropodSourceID",
      Label="Arthropod Source identifier",
      Definition="System-provided Arthropod Source identifier",
      Type="Integer",
      Mandatory="System-assigned"), 
    
    data.frame(
      Variable="ArthropodSourceSex",
      Label="Arthropod Source sex",
      Definition="Sex of the Arthropod Source",
      Type="Single selection",
      Mandatory="Yes"),
    
    data.frame(
      Variable="ArthropodSourceDateOfDeath",
      Label="Arthropod Source death date",
      Definition="Date the Arthropod Source died or was found dead",
      Type="Date",
      Mandatory="No"),
    
    data.frame(
      Variable="ArthropodSourceAnyPreviousMarkingCode",
      Label="Previous marking code",
      Definition="Answer to the question: 'Did the Arthropod Source have any previous marking?'",
      Type="Boolean",
      Mandatory="Yes"),
    
    data.frame(
      Variable="ArthropodSourcePreviousMarkingCode",
      Label="Arthropod Source previous marking code",
      Definition="The previous marking code of the Arthropod Source",
      Type="String",
      Mandatory="Yes if 'Previous marking code' is TRUE"))

